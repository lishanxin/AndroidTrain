<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
>
    <EditText
        android:id="@+id/net_work_ops_edit_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/net_work_ops_send_button"
        android:onClick="getHttpResponse"
        android:text="@string/net_work_ops_get"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/net_work_settings"
        android:onClick="netWorkSettings"
        android:text="@string/net_work_settings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/net_work_ops_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <WebView
        android:id="@+id/net_work_ops_web_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></WebView>
</LinearLayout>
